        "mu_hat = residuals.mean()\n",
        "sigma_hat = residuals.std()\n",
        "\n",
        "x_vals = np.linspace(residuals.min(), residuals.max(), 200)\n",
        "plt.hist(residuals, bins=20, density=True)\n",
        "plt.plot(x_vals, norm.pdf(x_vals, mu_hat, sigma_hat), color=\"red\")\n",
        "plt.title(\"Residuals with Fitted Normal Density\")\n",
        "plt.show()\n"
