      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from scipy.stats import norm\n",
        "\n",
        "mu, sigma = 0, 1\n",
        "\n",
        "p1 = norm.cdf(1, mu, sigma)\n",
        "p2 = norm.cdf(-0.5, mu, sigma)\n",
        "\n",
        "print(\"P(X ≤ 1) =\", p1)\n",
        "print(\"P(X ≤ -0.5) =\", p2)\n"
      ]
    },
    {
      "cell_type": "markdown",
      "metadata": {
        "id": "RsfbILFhWnVE"
